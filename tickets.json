{
  "openapi": "3.0.0",
  "info": {
    "title": "Tickets",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  },
  "paths": {
    "/tickets/book": {
      "post": {
        "tags": [
          "default"
        ],
        "summary": "Add Ticket",
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "userEmail": {
                    "type": "string"
                  },
                  "trainName": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/tickets/validate": {
      "post": {
        "tags": [
          "default"
        ],
        "summary": "Validate Ticket",
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "properties": {
                  "start_station": {
                    "type": "string",
                    "example": "Paris"
                  },
                  "end_station": {
                    "type": "string",
                    "example": "Caen"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "ticketId",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "example": "63b9b747c7eeb5682e9ab47c"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/tickets/find": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "Find Tickets",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    }
  }
}